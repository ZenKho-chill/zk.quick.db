{
  "name": "zk.quick.db",
  "version": "8.0.0",
  "description": "Thư viện trừu tượng hóa cơ sở dữ liệu mạnh mẽ, thống nhất và an toàn kiểu cho Node.js, hỗ trợ nhiều driver cơ sở dữ liệu với API nhất quán",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "database",
    "db",
    "sqlite",
    "mysql",
    "postgresql",
    "mongodb",
    "cassandra",
    "json",
    "memory",
    "typescript",
    "orm",
    "quick-db",
    "unified-api",
    "multi-database"
  ],
  "author": {
    "name": "ZenKho-chill",
    "url": "https://github.com/ZenKho-chill"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZenKho-chill/zk.quick.db.git"
  },
  "bugs": {
    "url": "https://github.com/ZenKho-chill/zk.quick.db/issues"
  },
  "homepage": "https://github.com/ZenKho-chill/zk.quick.db#readme",
  "scripts": {
    "test:all": "node tests/run-tests.js all",
    "test:integration": "node tests/run-tests.js integration",
    "test:unit": "node tests/run-tests.js unit",
    "test": "npm run test:unit",
    "test:watch": "jest --watch --testMatch='**/tests/unit/**/*.test.ts'",
    "test:coverage": "jest --coverage --testMatch='**/tests/**/*.test.ts'",
    "lint": "eslint --ext \".ts\" ./src",
    "prettier": "prettier --write ./src",
    "prettier:all": "prettier --write ./",
    "docgen": "typedoc",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf ./out",
    "prepare": "npm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-angular": "19.8.1",
    "@faker-js/faker": "10.0.0",
    "@types/better-sqlite3": "7.6.13",
    "@types/jest": "^30.0.0",
    "@types/lodash": "4.17.20",
    "@types/pg": "8.15.5",
    "@types/write-file-atomic": "4.0.3",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "dotenv": "17.2.1",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "git-cliff": "2.10.0",
    "jest": "30.0.5",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "lefthook": "1.12.3",
    "prettier": "3.6.2",
    "ts-jest": "29.4.1",
    "typedoc": "0.28.10",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "better-sqlite3": "^12.2.0",
    "cassandra-driver": "^4.8.0",
    "fs": "^0.0.1-security",
    "mongoose": "^8.18.0",
    "mysql2": "^3.14.3",
    "pg": "^8.16.3",
    "write-file-atomic": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "better-sqlite3": {
      "optional": true
    },
    "mongoose": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "write-file-atomic": {
      "optional": true
    },
    "cassandra-driver": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "exports": {
    ".": {
      "default": "./out/index.js",
      "types": "./out/index.d.js"
    },
    "./*": {
      "default": "./out/driver/*.js",
      "types": "./out/driver/*.d.ts"
    }
  },
  "lint-staged": {
    "*.{ts}": "eslint"
  }
}
